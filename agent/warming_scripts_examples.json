    {
      "name": "Login Test - Facebook",
      "description": "Test de login en Facebook",
      "category": "authentication",
      "duration_minutes": 5,
      "actions": [
        {
          "type": "navigate",
          "params": {
            "url": "https://www.facebook.com"
          }
        },
        {
          "type": "wait",
          "params": {
            "duration": 2
          }
        },
        {
          "type": "login",
          "params": {
            "service": "generic",
            "username": "your-email@example.com",
            "password": "your-password",
            "username_selector": "input[name='email']",
            "password_selector": "input[name='pass']",
            "submit_selector": "button[name='login']"
          }
        },
        {
          "type": "wait",
          "params": {
            "duration": 5
          }
        },
        {
          "type": "scroll",
          "params": {
            "direction": "down",
            "amount": 300
          }
        }
      ]
    },
    {
      "name": "Complex Form Filling",
      "description": "Llenado complejo de formulario",
      "category": "forms",
      "duration_minutes": 10,
      "actions": [
        {
          "type": "navigate",
          "params": {
            "url": "https://example.com/contact"
          }
        },
        {
          "type": "wait",
          "params": {
            "duration": 2
          }
        },
        {
          "type": "human_typing",
          "params": {
            "selector": "input[name='name']",
            "text": "John Doe",
            "error_rate": 0.03
          }
        },
        {
          "type": "wait",
          "params": {
            "duration": 1
          }
        },
        {
          "type": "human_typing",
          "params": {
            "selector": "input[name='email']",
            "text": "john.doe@example.com",
            "error_rate": 0.02
          }
        },
        {
          "type": "wait",
          "params": {
            "duration": 1
          }
        },
        {
          "type": "select",
          "params": {
            "selector": "select[name='country']",
            "value": "US"
          }
        },
        {
          "type": "wait",
          "params": {
            "duration": 1
          }
        },
        {
          "type": "human_typing",
          "params": {
            "selector": "textarea[name='message']",
            "text": "This is a test message with some content.",
            "error_rate": 0.05
          }
        },
        {
          "type": "wait",
          "params": {
            "duration": 2
          }
        },
        {
          "type": "random_mouse",
          "params": {
            "movements": 2
          }
        }
      ]
