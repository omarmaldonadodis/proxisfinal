{
  "warming_scripts_examples": [
    {
      "name": "Google Search Warm-up",
      "description": "Script básico de búsqueda en Google",
      "category": "search",
      "duration_minutes": 10,
      "actions": [
        {
          "type": "navigate",
          "params": {
            "url": "https://www.google.com"
          },
          "description": "Navegar a Google"
        },
        {
          "type": "wait",
          "params": {
            "duration": 2
          }
        },
        {
          "type": "search_google",
          "params": {
            "query": "latest technology news"
          },
          "description": "Buscar noticias de tecnología"
        },
        {
          "type": "wait",
          "params": {
            "duration": 3
          }
        },
        {
          "type": "scroll",
          "params": {
            "direction": "down",
            "amount": 300,
            "smooth": true
          }
        },
        {
          "type": "wait",
          "params": {
            "duration": 2
          }
        },
        {
          "type": "random_mouse",
          "params": {
            "movements": 3
          }
        }
      ]
    },
